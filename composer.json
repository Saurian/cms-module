{
	"name": "venne/cms-module",
	"description": "CMS module with administration",
	"keywords": ["cms", "nette", "venne", "module"],
	"version": "2.0.0",
	"authors": [
		{
			"name": "Josef Kříž",
			"homepage": "http://josef-kriz.cz"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"venne/doctrine-module": "2.0.x-dev",
		"venne/assets-module": "2.0.x-dev",
		"venne/translator-module": "2.0.x-dev",
		"venne/gedmo-module": "2.0.x-dev",
		"kdyby/bootstrap-form-renderer": "v1.1.0",
		"o5/grido": "@dev"
	},
	"require-dev": {
		"venne/tester": "@dev"
	},
	"autoload": {
		"psr-0": {
			"CmsModule": ""
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0.x-dev"
		},
		"venne": {
			"configuration": {
				"parameters": {
					"administration": {
						"login": {
							"name": "",
							"password": ""
						},
						"routePrefix": "admin",
						"defaultPresenter": "Dashboard"
					},
					"website": {
						"name": "Blog",
						"title": "%n %s %t",
						"titleSeparator": "|",
						"keywords": "",
						"description": "",
						"author": "",
						"routePrefix": "",
						"oneWayRoutePrefix": "",
						"languages": {},
						"defaultLanguage": "cs",
						"defaultPresenter": "Homepage",
						"errorPresenter": "Cms:Error",
						"layout": "@cms/bootstrap",
						"cacheMode": "",
						"cacheValue": "10"
					}
				},
				"extensions": {
					"cms": "CmsModule\\DI\\CmsExtension"
				}
			}
		}
	},
	"minimum-stability": "dev"
}