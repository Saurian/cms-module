{
	"name": "venne/cms-module",
	"description": "CMS module with administration",
	"keywords": ["cms", "nette", "venne", "module"],
	"version": "2.1.0",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Josef Kříž",
			"homepage": "http://josef-kriz.cz"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"venne/doctrine-module": "2.1.*",
		"venne/deployment-module": "2.1.*",
		"venne/assets-module": "2.1.*",
		"venne/translator-module": "2.1.*",
		"venne/bootstrap3-module": "*",
		"venne/bootstrapmodal-module": "2.1.*",
		"venne/jquery2.0-module": "*",
		"venne/jqueryui1.10-module": "*",
		"venne/select2-module": "3.4.*",
		"venne/typeahead-module": "0.9.*",
		"venne/dynatree-module": "1.2.*",
		"venne/stringtoslug-module": "1.2.*",
		"venne/holder-module": "2.0.*",
		"venne/bootstrapdatetime-module": "0.0.*",
		"venne/grido-module": "2.0.*",
		"venne/jqueryfileupload-module": "8.8.*",
		"kdyby/bootstrap-form-renderer": "v2.0.0",
		"venne/nette-ajax": "dev-venne"
	},
	"require-dev": {
		"venne/tester": "@dev"
	},
	"suggest": {
		"venne/blog-module": "Module with blog system",
		"venne/gallery-module": "Simple gallery page based on blog module",
		"venne/guestbook-module": "Simple guestbook page",
		"venne/mailform-module": "Mailform page with many useful options",
		"venne/googleanalytics-module": "Google analytics integration",
		"venne/ckeditor-module": "Popular WYSIWYG editor",
		"venne/ace-module": "Great code editor for templates",
		"venne/lightbox-module": "Lightbox integration",
		"venne/social-module": "Integration of logging with social networks"
	},
	"autoload": {
		"psr-0": {
			"CmsModule": ""
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.1-dev"
		},
		"venne": {
			"configuration": {
				"parameters": {
					"administration": {
						"login": {
							"name": "",
							"password": ""
						},
						"routePrefix": "admin",
						"defaultPresenter": "Dashboard"
					},
					"website": {
						"name": "Blog",
						"title": "%n %s %t",
						"titleSeparator": "|",
						"keywords": "",
						"description": "",
						"author": "",
						"routePrefix": "",
						"oneWayRoutePrefix": "",
						"languages": {},
						"defaultLanguage": "cs",
						"defaultPresenter": "Homepage",
						"errorPresenter": "Cms:Error",
						"layout": "@cms/bootstrap",
						"cacheMode": "",
						"cacheValue": "10",
						"theme": ""
					}
				},
				"extensions": {
					"cms": "CmsModule\\DI\\CmsExtension"
				}
			}
		}
	}
}